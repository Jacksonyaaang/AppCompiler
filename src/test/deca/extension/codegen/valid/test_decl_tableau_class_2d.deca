class Point2D {
    int x ; // Abscisse
    int y ; // Ordonnee
    // Deplace ce point
    // de a en diagonale.
    void diag(int a) {
        x = x + a;
        y = y + a;
    }
}

class Point3D extends Point2D {
    int z; // Hauteur
    void diag(int a) {
        x = x + a;
        y = y + a;
        z = z + a;
    }
}

